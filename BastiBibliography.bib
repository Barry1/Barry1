@Article{SLVO:jacdav,
  author =       "Gerald L. G. Sleijpen and Henk A. {Van der Vorst}",
  title =        "A Jacobi-Davidson Iteration Method for Linear
                 Eigenvalue Problems",
  journal =      "SIAM Review",
